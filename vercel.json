{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "page.tsx" },
    { "src": "/client/menu/(.*)", "dest": "/client/menu/[tableName]/page.tsx" }
  ]
}
